<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.prison_i">
    <uses-feature android:name="android.hardware.sensor.stepcounter"/>
    <uses-feature android:name="android.hardware.sensor.stepdetector"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".prisoner_Login_W"></activity>
        <activity android:name=".AdminSignUp" />
        <activity
            android:name=".Prisoners_NavDrawAct"
            android:label="@string/title_activity_prisoners__nav_draw"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".SignUp_Activity" />
        <activity android:name=".Login_Activity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>